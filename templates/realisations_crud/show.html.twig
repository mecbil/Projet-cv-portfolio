{% extends 'base.html.twig' %}

{% block title %}Realisations{% endblock %}

{% block body %}
    <h1>Realisations</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ realisation.id }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ realisation.image }}</td>
            </tr>
            <tr>
                <th>Link</th>
                <td>{{ realisation.link }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ realisation.content }}</td>
            </tr>
            <tr>
                <th>Color</th>
                <td>{{ realisation.color }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_realisations_crud_index') }}">back to list</a>

    <a href="{{ path('app_realisations_crud_edit', {'id': realisation.id}) }}">edit</a>

    {{ include('realisations_crud/_delete_form.html.twig') }}
